{
  "root": "build/",
  "clean_urls": true,
  "routes": {
    "/**": "index.html"
  },
  "https_only": true,
  "error_page": "index.html",
  "headers": {
    "/static/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/images/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/categories/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    }
  }
} 